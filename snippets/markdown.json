	"Add html tags: details": {
		"prefix": "details-tag",
		"body": [
			"<details>",
			"<summary>$1</summary>",
			"",
			"${2:content}",
			"",
			"</details>",
			"$0"
		],
		"description": "add html tags: details"
	},
	"Add html tags: span with red": {
