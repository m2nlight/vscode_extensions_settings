// Place your key bindings in this file to override the defaults
[
    // {
    //     "key": "ctrl+.",
    //     "command": "-editor.action.quickFix",
    //     "when": "editorHasCodeActionsProvider && textInputFocus && !editorReadonly"
    // },
    {
        "key": "alt+enter",
        "command": "editor.action.quickFix",
        "when": "editorHasCodeActionsProvider && textInputFocus && !editorReadonly"
    },
    {
        "key": "ctrl+shift+5",
        "command": "-coverage-gutters.toggleCoverage"
    },
    {
        "key": "ctrl+shift+6",
        "command": "-coverage-gutters.previewCoverageReport"
    },
    {
        "key": "ctrl+shift+7",
        "command": "-coverage-gutters.displayCoverage"
    },
    {
        "key": "ctrl+shift+8",
        "command": "-coverage-gutters.watchCoverageAndVisibleEditors"
    },
    {
        "key": "ctrl+shift+9",
        "command": "-coverage-gutters.removeWatch"
    },
    {
        "key": "ctrl+shift+backspace",
        "command": "-coverage-gutters.removeCoverage"
    },
    {
        "key": "ctrl+shift+v",
        "command": "-markdown.showPreview"
    },
    {
        "key": "ctrl+shift+v",
        "command": "-markdown-preview-enhanced.openPreview"
    },
    {
        "key": "ctrl+shift+v",
        "command": "-markdown.extension.closePreview"
    },
    {
        "key": "ctrl+shift+v",
        "command": "-quicktype.pasteJSONAsTypesAndSerialization"
    },
    {
        "key": "ctrl+shift+alt+v",
        "command": "-quicktype.pasteJSONAsTypes"
    },
    {
        "key": "ctrl+shift+s",
        "command": "-markdown-preview-enhanced.syncPreview"
    }
]